{
  "name": "nagme",
  "version": "1.0.0",
  "description": "NagMe is the super proactive task and reminder app",
  "main": "server.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.4",
    "pg": "^7.12.1",
    "string-hash": "^1.1.3",
    "superagent": "^5.1.0"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "create-tables": "node data/create-tables.js",
    "drop-tables": "node data/drop-tables.js",
    "load-seed-data": "node data/load-seed-data.js",
    "recreate-tables": "npm run drop-tables && npm run create-tables",
    "setup-db": "npm run recreate-tables && npm run load-seed-data",
    "setup-db:prod": "heroku run npm run setup-db",
    "setup-heroku": "heroku addons:create heroku-postgresql:hobby-dev && heroku config:set PGSSLMODE=require"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotPush/nagme.git"
  },
